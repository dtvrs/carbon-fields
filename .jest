{
	"bail": true,
	"verbose": true,

	"testPathDirs": [
		"assets/tests"
	],

	"unmockedModulePathPatterns": [
		"<rootDir>/node_modules/react/",
		"<rootDir>/node_modules/enzyme/"
	],

	"modulePaths": [
		"<rootDir>/assets/react"
	],

	"moduleNameMapper": {
		"^tests/helpers$": "<rootDir>/assets/tests/helpers.js"
	}
}
